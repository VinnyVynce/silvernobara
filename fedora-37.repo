[fedora-37]
name=Fedora 37 $basearch
mirrorlist=https://mirrors.fedoraproject.org/metalink?repo=fedora-37&arch=$basearch
enabled=1
gpgcheck=1
metadata_expire=1d
exclude=kernel*,mesa,mesa-libGLU,vulkan-headers,vulkan-loader,vulkan-tools,libdrm,libglvnd,mutter,gnome-control-center,mangohud
exclude_from_weak=gnome-tour