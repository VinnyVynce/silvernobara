[fedora-36-updates]
name=Fedora 36 $basearch Updates
mirrorlist=https://mirrors.fedoraproject.org/metalink?repo=updates-released-f36&arch=$basearch
enabled=1
gpgcheck=1
metadata_expire=1d
exclude=kernel*,mesa,libdrm,libglvnd,mesa-libGLU,vulkan-headers,vulkan-loader,vulkan-tools,mutter,gnome-control-center,mangohud
exclude_from_weak=gnome-tour
